// MINALIN, RENZ G.              DIT 1-2
#include <stdio.h>

int main() {

    int i = 1;

    printf("Print numbers 1 to 10 using while loop :\n");
    while (i <= 10) {
        printf("%d\n", i);
        i++;
    }

    printf("\nfor loop (Even numbers 2 to 20): \n");
    for (i = 2; i <= 20; i += 2) {
        printf("%d\n", i);
    }

    printf("\nDo-while loop (1 to 5): \n");
    i = 1;
    do {
        printf("%d\n", i);
        i++;
    }
    while (i <= 5);

    return 0;
}
